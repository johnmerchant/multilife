{
  "name": "multilife",
  "version": "1.2.0",
  "description": "Multiplayer game of life",
  "main": "server.js",
  "repository": "https://github.com/multilife",
  "author": "John Merchant <john@merchant.on.net>",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "blob-to-buffer": "^1.2.8",
    "color": "^3.1.2",
    "color-namer": "^1.4.0",
    "dotenv": "^8.2.0",
    "immer": "^5.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-github-corner": "^2.3.0",
    "react-redux": "^7.1.3",
    "react-share": "^3.0.1",
    "redux": "^4.0.4",
    "ws": "^7.2.0"
  },
  "scripts": {
    "test": "tsc && jest",
    "clean": "rimraf dist/",
    "build-client": "parcel build client/index.html --out-dir dist/client",
    "dev-client": "parcel serve client/index.html --out-dir dist/client",
    "build-server": "tsc --outDir dist/server",
