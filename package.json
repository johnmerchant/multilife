{
  "name": "multilife",
  "version": "1.3.0",
  "description": "Multiplayer game of life",
  "main": "server.js",
  "repository": "https://github.com/jmercha/multilife",
  "author": "John Merchant <john@jmercha.dev>",
  "license": "MIT", 
  "scripts": {
    "test": "tsc && jest",
    "clean": "rimraf dist/",
    "build-client": "parcel build client/index.html --out-dir dist/client",
    "dev-client": "parcel serve client/index.html --out-dir dist/client",
    "build-server": "tsc --outDir dist/server",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "dev-server": "npm run clean && npm run build-server && cross-env HTTP_PORT=5000 node dist/server/server"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "blob-to-buffer": "^1.2.8",
    "color": "^3.1.2",
    "color-namer": "^1.4.0",
    "dotenv": "^8.2.0",
    "immer": "^7.0.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-github-corner": "^2.3.0",
    "react-redux": "^7.2.0",
    "react-share": "^4.2.0",
    "redux": "^4.0.5",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@types/blob-to-buffer": "^1.2.0",
    "@types/color": "^3.0.0",
    "@types/color-namer": "^1.3.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-share": "^3.0.2",
    "@types/ws": "^7.2.5",
    "cross-env": "^7.0.2",
    "jest": "^26.0.1",
    "parcel": "^1.12.4",
    "rimraf": "^3.0.0",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  }
}
